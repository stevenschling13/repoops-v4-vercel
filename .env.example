# Environment Variables Template
# Copy this file to .env.local and fill in your values

# Public Environment Variables (exposed to browser)
NEXT_PUBLIC_API_URL=https://api.opsdeck.dev

# Vercel Configuration (automatically set in production)
VERCEL_ANLYTICS_ID=your-analytics-id-here

# Feature Flags
FEATURE_FLAG_EXPERIMENTAL=false

# Server-Side Only Variables (NOT exposed to browser)
API_SECRET_KEY=your-api-key-here

# Development Only
NEXT_TELEEMRY_DISABLED=1
